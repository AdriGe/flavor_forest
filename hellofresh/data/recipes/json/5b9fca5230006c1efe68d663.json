            ],
            "images": [
                {
                    "link": "https://d3hvwccx09j84u.cloudfront.net/0,0/5b9fca5230006c1efe68d663/step-2d2d1843.jpg",
                    "path": "/5b9fca5230006c1efe68d663/step-2d2d1843.jpg",
